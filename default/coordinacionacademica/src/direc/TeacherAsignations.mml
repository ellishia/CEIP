package direc {

	["mbdn_ew"]
	definition TeacherAsignations is collection {
		label = "Asignaciones de Horas";
		is-singleton;
		index = ref Indice;
		add {
			node = ref TeacherAsignation;
		}

		["imz72kgw"]
		definition Indice is index {
			label = "Indice";
			reference {

				["im69logq"]
				attribute Teacher {
					label = "Docente";
					type = LINK;
				}
			}

			["mdckyyw"]
			view Normal {
				show {
					title = ref Teacher;
				}
			}

		}

		["mxunqha"]
		view {
			label = "Todos";
			show {
				index {
					sort-by = ref Indice.Teacher;
					with-view = ref Indice.Normal;
				}
			}
			analyze {
				dimension {
					attribute = ref Indice.Teacher;
				}
				sorting {
					attribute = ref Indice.Teacher;
				}
			}
		}

		["m8jehqa"]
		definition TeacherAsignation is form {
			label = "Asignaci√≥n Horaria";

			["mz4m8ka"]
			field-date From {
				label = "Desde";
				precision = DAYS;
			}

			["mhzbcfg"]
			field-date To {
				label = "Hasta";
				precision = DAYS;
			}

			["mmf1h9w"]
			field-link Teacher {
				label = "Docente";
				source {
					index = ref Teachers.Indice;
					collection = ref Teachers;
				}
			}
			mapping {
				index = ref Indice;
				//calculate() {		}
			}

			["mc24t5q"]
			view {
				label = "Ficha";
				show {
					field = ref From;
					field = ref To;
					field = ref Teacher;
				}
			}

		}

	}
}